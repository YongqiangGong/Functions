expr_numeric <- function(expr){
rownames <- rownames(expr)
expr <- apply(expr,2,as.numeric)
row.names(expr) <- rownames
return(expr)
}
expr <- data.frame(x=1:6,y=3:9)
expr <- data.frame(x=1:6,y=3:8)
View(expr)
fff=expr_numeric(expr)
View(fff)
View(fff)
View(fff)
